library(stats)
Outliers_Pox_Plot <- function(h,k){
if(is.numeric(h[1])){
k=1
l=c()
b=length(h)
d=c(quantile(h,na.rm=TRUE))
d=as.numeric(d)
LIF=d[2]-k*(d[4]-d[2]) #1er quartile  : d[2]
UIF=d[2]+k*(d[4]-d[2]) #3eme quartile : d[4]
for(j in 1:b)
{ if(is.na(h[j])==0){
if(h[j] < LIF || h[j] > UIF) h[j]=NA}}
}
return(h)
}
